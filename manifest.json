{
    "manifest_version": 3,
    "name": "禁用搜索建议",
    "version": "1.0",
    "description": "隐藏Chrome地址栏的搜索建议",
    "permissions": [
      "history",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_icon": "icon.png"
    }
  }
  